<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<LangVersion>8.0</LangVersion>
	<Nullable>enable</Nullable>   
    <Title>SAM.T.Protocol</Title>
	<Version>1.2.3</Version>
	<PackageId>SAM.T.Protocol</PackageId>
    <Company>unwild</Company>
    <Authors>unwild</Authors>
    <Description>Models exchanged between SAM.T agent and client</Description>
    <Copyright>Copyright (c) 2024 Hugo R</Copyright>
    <PackageProjectUrl>https://github.com/unwild/SAM.T</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/unwild/SAM.T</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
